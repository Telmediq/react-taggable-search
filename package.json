{
  "name": "react-taggable-search",
  "version": "1.0.0",
  "description": "React Taggable Search",
  "main": "lib/ReactTaggableSearch.js",
  "author": "Tyson Battistella",
  "homepage": "https://github.com/telmediq/react-taggable-search",
  "repository": {
    "type": "git",
    "url": "https://github.com/telmediq/react-taggable-search.git"
  },
  "bugs": {
    "url": "https://github.com/telmediq/react-taggable-search/issues"
  },
  "dependencies": {
    "classnames": "^2.1.2",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react-redux": "^5.0.5",
    "react-select": "^1.0.0-rc.5",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.5.2"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^1.6.0",
    "eslint-plugin-react": "^3.5.1",
    "gulp": "^3.9.0",
    "jest": "^20.0.4",
    "react": "^0.14.0",
    "react-component-gulp-tasks": "^0.7.6",
    "react-dom": "^0.14.0"
  },
  "peerDependencies": {
    "react": "^0.14.0",
    "react-select": "^1.0.0-rc.5",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "browserify-shim": {
    "react": "global:React"
  },
  "scripts": {
    "build": "gulp clean && NODE_ENV=production gulp build",
    "examples": "gulp dev:server",
    "lint": "eslint ./; true",
    "publish:site": "NODE_ENV=production gulp publish:examples",
    "release": "NODE_ENV=production gulp release",
    "start": "gulp dev",
    "test": "jest",
    "watch": "gulp watch:lib"
  },
  "keywords": [
    "react",
    "react-component"
  ],
  "jest": {
    "moduleNameMapper": {
      "moduleNameMapper": {
        "^.+\\.js$": "babel-jest",
        "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
        "^.+\\.(css|less)$": "identity-obj-proxy"
      },
      "moduleFileExtensions": [
        "js",
        "jsx"
      ],
      "moduleDirectories": [
        "node_modules"
      ],
      "testPathIgnorePatterns": [
        "<rootDir>/(public|node_modules)/"
      ],
      "transformIgnorePatterns": [
        "<rootDir>/node_modules/(?!binary-type-tree)"
      ],
      "transform": {
        "^.+\\.(js|ts)$": "<rootDir>/spec/preprocessor.js"
      }
    }
  }
}
